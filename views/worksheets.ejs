<% include partials/boilerplate %>
<body class="worksheet_container">
<% include partials/header %>

<main class="wrapper">
  <h1>Worksheets</h1>

  <% worksheets.forEach((worksheet) => { %>
    <div class="worksheet">
      <h2><%= worksheet.name %></h2>
      <p><%= worksheet.subject %></p>
      <form action="/worksheets/<%=worksheet.id%>" method="post">
        <button class="awd-btn">Add to Workspace</button>
      </form>
    </div>
  <% }) %>
</main>

<% include partials/end %>
